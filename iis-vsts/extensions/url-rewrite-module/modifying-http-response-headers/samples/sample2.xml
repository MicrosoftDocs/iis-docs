<?xml version="1.0" encoding="UTF-8"?>
<configuration>
<system.webServer>
  <rewrite>
    <rules>
     <rule name="Redirect" stopProcessing="true">
     <match url="^index\.aspx$" />
     <action type="Redirect" url="default.aspx" />
    </rule>
   </rules>
  </rewrite>
</system.webServer>
</configuration>