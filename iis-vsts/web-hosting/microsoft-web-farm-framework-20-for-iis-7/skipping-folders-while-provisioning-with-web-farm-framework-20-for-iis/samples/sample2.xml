		<applicationProvision syncWebServerFromPrimary="true">
			<skipDirectives>
				<skip name="folder1" skipDirective="objectName=dirPath,absolutePath=.*folder1.*" />
			</skipDirectives>
		</applicationProvision>
	</webFarm>
</webFarms>