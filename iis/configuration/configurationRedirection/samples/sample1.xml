<CONFIGURATION>

   <CONFIGSECTIONS>
      <SECTION name="configurationRedirection" />
   </CONFIGSECTIONS>

   <CONFIGPROTECTEDDATA>
      <PROVIDERS>
         <ADD description="Uses RsaCryptoServiceProvider to encrypt and decrypt" name="IISRsaProvider" type="" keyContainerName="iisConfigurationKey" cspProviderName="" useMachineContainer="true" useOAEP="false" />
      </PROVIDERS>
   </CONFIGPROTECTEDDATA>

<CONFIGURATIONREDIRECTION enabled="true" path="\\ServerName\ShareName" userName="MyUser" password="[enc:IISRsaProvider:***:enc]" />

</CONFIGURATION>
