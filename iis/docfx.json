{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.exe",
          "**/*.wmv",
          "**/*.zip",
          "**/*.bmp",
          "**/*.jpeg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/iis/breadcrumb/toc.json",
      "_navPath": "/foo",
      "_navRel": "/foo",
      "searchScope": ["IIS"],
      "ms.prod": "iis",
      "ms.topic": "conceptual",
      "author": "shirhatti",
      "ms.author": "soshir"
    },
    "fileMetadata": {
      "ms.technology": {
        "application-frameworks/**/*.md": "iis-appfx",
        "configuration/**/*.md": "iis-config",
        "develop/**/*.md": "iis-develop",
        "extensions/**/*.md": "iis-extensions",
        "manage/**/*.md": "iis-manage",
        "media/**/*.md": "iis-media",
        "publish/**/*.md": "iis-publish",
        "troubleshoot/**/*.md": "iis-troubleshoot",
        "web-hosting/**/*.md": "iis-hosting"
      },
      "ms.topic": {
        "configuration/**/*.md": "reference",
        "extensions/express-api-reference/**/*.md": "reference",
        "ftp-extensibility-reference/native-code-api-reference/**/*.md": "reference",
        "web-development-reference/native-code-api-reference/**/*.md": "reference",
        "extensions/database-manager-reference/**/*.md": "managed-reference",
        "wmi-provider/**/*.md": "reference"
      }      
    },
    "template": [],
    "dest": "IIS"
  }
}